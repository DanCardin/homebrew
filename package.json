{
  "name": "homebrew",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "fix": "vue-cli-service lint --fix",
    "lint": "vue-cli-service lint --max-warnings=0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
